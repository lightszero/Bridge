{
  "output": "Bridge\\output",
  "cleanOutputFolderBeforeBuildPattern": "*.*",
  "outputBy": "Namespace",
  "outputFormatting": "Formatted",
  "fileNameCasing": "CamelCase",
  // console setting has precedence over resources
  "console": true,
  "resources":[
    {
      "name": "bridge.console.js",
      "extract": false
    }
  ],
  "sourceMap": {
    "enabled": true,
    "eol": "crlf"
  },
  "logging": {
    "level": "None",
    "folder": "../../$(OutputPath)" // output to the Integration Test common log
  },
  "report": "bridge.report.log"
}